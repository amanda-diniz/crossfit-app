generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}


model PR {
  id       Int     @id @default(autoincrement())
  exercise String?
  date DateTime @db.Date
  weight Decimal     
}


model Repetition {
  id       Int     @id @default(autoincrement())
  exercise String?
  date DateTime @db.Date
  repetition Int     
}

